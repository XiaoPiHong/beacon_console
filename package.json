{
	"name": "beacon_console",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build:test": "tsc && vite build --mode test",
		"build:prod": "tsc && vite build --mode production",
		"build:gh-pages": "tsc && vite build --mode gh-pages",
		"preview": "vite preview",
		"lint:create": "eslint --init",
		"lint": "eslint \"src/**/*.{js,tsx,ts}\" --fix",
		"prettier-format": "prettier --config .prettierrc.cjs \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\" --write",
		"lint:css": "stylelint **/*.{less,css,scss} --fix",
		"prepare": "husky install",
		"commit": "cz"
	},
	"dependencies": {
		"@ant-design/icons": "^5.2.6",
		"antd": "^5.14.2",
		"dayjs": "^1.11.10",
		"keepalive-for-react": "^2.0.7",
		"lodash-es": "^4.17.21",
		"qs": "^6.11.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.1.3",
		"react-router-dom": "^6.17.0",
		"redux": "^4.2.1",
		"redux-promise": "^0.6.0",
		"xph-form": "^1.3.6"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.5.1",
		"@commitlint/config-conventional": "^17.4.4",
		"@types/lodash-es": "^4.17.10",
		"@types/qs": "^6.9.10",
		"@types/react": "^18.2.37",
		"@types/react-dom": "^18.2.15",
		"@types/react-redux": "^7.1.28",
		"@types/react-router-dom": "^5.3.3",
		"@types/redux-promise": "^0.5.32",
		"@typescript-eslint/eslint-plugin": "^5.57.0",
		"@typescript-eslint/parser": "^5.57.0",
		"@vitejs/plugin-react": "^3.1.0",
		"commitizen": "^4.3.0",
		"cz-customizable": "^7.0.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"less": "^4.1.3",
		"lint-staged": "^13.2.0",
		"prettier": "^2.8.7",
		"redux-devtools-extension": "^2.13.9",
		"stylelint": "^14.9.1",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-recess-order": "^3.1.0",
		"stylelint-config-standard": "^25.0.0",
		"stylelint-less": "^1.0.6",
		"typescript": "^4.9.5",
		"vite": "^4.2.0",
		"vite-plugin-eslint": "^1.8.1"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"npm run lint",
			"npm run prettier-format"
		],
		"*.{css,sass,scss,less}": [
			"npm run lint:css"
		]
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": ".cz-config.cjs"
		}
	}
}
